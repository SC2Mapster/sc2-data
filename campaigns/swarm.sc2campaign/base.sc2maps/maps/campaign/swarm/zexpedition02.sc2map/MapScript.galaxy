    PlayerGroupLoopEnd();
    Wait(420.0, c_timeAI);
    PlayerGroupLoopBegin(gv_protossPlayers);
    while (!PlayerGroupLoopDone()) {
        TechTreeUpgradeAddLevel(PlayerGroupLoopCurrent(), "ProtossShieldsLevel1", 1);
        PlayerGroupLoopStep();
    }
    PlayerGroupLoopEnd();
    return true;
