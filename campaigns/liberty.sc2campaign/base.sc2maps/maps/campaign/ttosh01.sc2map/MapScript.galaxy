        TriggerExecute(gt_TransmissionMineralNodeReveal1Q, true, true);
    }
    else if (autoB1F26F62_val == 3) {
        PlayerGroupLoopBegin(gv_zergPlayers);
        while (!PlayerGroupLoopDone()) {
            TechTreeUpgradeAddLevel(PlayerGroupLoopCurrent(), "ZergMeleeWeaponsLevel1", libNtve_gf_DifficultyValueInt(0, 0, 1, 0));
            TechTreeUpgradeAddLevel(PlayerGroupLoopCurrent(), "ZergMissileWeaponsLevel1", libNtve_gf_DifficultyValueInt(0, 0, 1, 0));
            TechTreeUpgradeAddLevel(PlayerGroupLoopCurrent(), "ZergFlyerWeaponsLevel1", libNtve_gf_DifficultyValueInt(0, 0, 1, 0));
            PlayerGroupLoopStep();
        }
        PlayerGroupLoopEnd();
        Wait((gv_lavaInterval * 0.8), c_timeGame);
        TriggerExecute(gt_TransmissionMineralNodeReveal2Q, true, true);
    }
    else if (autoB1F26F62_val == 4) {
        PlayerGroupLoopBegin(gv_zergPlayers);
        while (!PlayerGroupLoopDone()) {
            TechTreeUpgradeAddLevel(PlayerGroupLoopCurrent(), "ZergMeleeWeaponsLevel2", libNtve_gf_DifficultyValueInt(0, 0, 0, 1));
