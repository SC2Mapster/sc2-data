    Wait(60.0, c_timeAI);
    AIAttackWaveAddUnits4(2, 2, 3, 4, "Mutalisk");
    AIAttackWaveAddWaypoint(gv_p08_ZERG_SOUTH, PointFromId(1117), false);
    AIAttackWaveSend(gv_p08_ZERG_SOUTH, 100, true);
    while (true) {
        Wait(120.0, c_timeAI);
