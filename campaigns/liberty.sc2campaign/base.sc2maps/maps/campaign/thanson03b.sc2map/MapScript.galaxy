        return true;
    }

    while (true) {
        if ((UnitGroupCount(gv_carrionBirds, c_unitCountAlive) == 0)) {
            return true;
        }
        else {
        }
        UnitGroupLoopBegin(gv_carrionBirds);
        while (!UnitGroupLoopDone()) {
            if ((UnitGroupCount(UnitGroup(null, gv_p01_USER, RegionCircle(UnitGetPosition(UnitGroupLoopCurrent()), 5.0), UnitFilter(0, 0, (1 << c_targetFilterAir) | (1 << c_targetFilterMissile) | (1 << c_targetFilterCloaked), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 0), c_unitCountAlive) >= 1)) {
                UnitGroupRemove(gv_carrionBirds, UnitGroupLoopCurrent());
                gf_OrderCarrionBirdtoFlyAway(UnitGroupLoopCurrent());
            }
            else {
            }
            UnitGroupLoopStep();
        }
        UnitGroupLoopEnd();
        Wait(1.0, c_timeGame);
    }
    return true;
}

