        Wait(150.0, c_timeAI);
        AIAttackWaveAddUnits4(10, 12, 14, 16, "InfestedCivilian");
        AIAttackWaveAddUnits4(3, 5, 7, 9, "InfestedTerranCampaign");
        AIAttackWaveAddWaypoint(gv_p02_ZERG_EAST, PointFromId(76), false);
