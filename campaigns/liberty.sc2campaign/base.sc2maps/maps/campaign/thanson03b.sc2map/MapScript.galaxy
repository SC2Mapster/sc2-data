    AIAttackWaveAddUnits4(6, 6, 5, 9, "Zergling");
    AIAttackWaveAddUnits4(0, 0, 1, 1, "Hydralisk");
    AIAttackWaveAddWaypoint(gv_p04_ZERG_NORTH, PointFromId(591), false);
