    }

    AIAttackWaveSetTargetPlayer(gv_p02_ZERG_EAST, PlayerGroupSingle(gv_p01_USER));
    AIAttackWaveSetTargetPlayer(gv_p04_ZERG_NORTH, PlayerGroupSingle(gv_p01_USER));
