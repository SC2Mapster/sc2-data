//--------------------------------------------------------------------------------------------------
// Trigger: Photon Cannon reveal
//--------------------------------------------------------------------------------------------------
bool gt_PhotonCannonreveal_Func (bool testConds, bool runActions) {
    unitgroup auto4FBC37CA_g;
    int auto4FBC37CA_u;

    // Variable Declarations
    unit lv_dTs;
    unitgroup lv_closeCannons;

    // Variable Initialization
    lv_dTs = null;
    lv_closeCannons = UnitGroupEmpty();

    // Conditions
    if (testConds) {
        if (!((gv_gameOver == false))) {
            return false;
        }

        if (!((gv_inCinematic == false))) {
            return false;
        }
    }

    // Actions
    if (!runActions) {
        return true;
    }

    auto4FBC37CA_g = gv_gROUPEscapees;
    auto4FBC37CA_u = 1;
    while (auto4FBC37CA_u <= UnitGroupCount(auto4FBC37CA_g, c_unitCountAll)) {
        lv_dTs = UnitGroupUnit(auto4FBC37CA_g, auto4FBC37CA_u);
        UnitGroupLoopBegin(UnitGroup("PhotonCannon", c_playerAny, RegionCircle(UnitGetPosition(lv_dTs), 11.0), UnitFilter(0, 0, (1 << c_targetFilterMissile), (1 << (c_targetFilterDead - 32)) | (1 << (c_targetFilterHidden - 32))), 0));
        while (!UnitGroupLoopDone()) {
            UnitGroupAdd(lv_closeCannons, UnitGroupLoopCurrent());
            UnitGroupLoopStep();
        }
        UnitGroupLoopEnd();
        if (lv_dTs == UnitGroupUnit(auto4FBC37CA_g, auto4FBC37CA_u)) {
            auto4FBC37CA_u = auto4FBC37CA_u + 1;
        }
    }
    UnitGroupLoopBegin(lv_closeCannons);
    while (!UnitGroupLoopDone()) {
        VisRevealArea(gv_pLAYER_01_USER, RegionCircle(UnitGetPosition(UnitGroupLoopCurrent()), 2.0), 1.1, false);
        UnitGroupLoopStep();
    }
    UnitGroupLoopEnd();
    return true;
}

