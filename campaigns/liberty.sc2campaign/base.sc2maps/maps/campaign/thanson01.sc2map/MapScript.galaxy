//--------------------------------------------------------------------------------------------------
// Trigger: Tumor Spawning Trigger
//--------------------------------------------------------------------------------------------------
bool gt_TumorSpawningTrigger_Func (bool testConds, bool runActions) {
    // Variable Declarations
    unit lv_nydusWorm;
    point lv_point;

    // Variable Initialization
    lv_nydusWorm = null;
    lv_point = null;

    // Actions
    if (!runActions) {
        return true;
    }

    lv_point = gv_tempTumorPoint;
