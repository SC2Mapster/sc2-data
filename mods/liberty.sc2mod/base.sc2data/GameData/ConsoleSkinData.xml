        <FeaturedImage value="Assets/Textures/SC2_UI_Collection_HUDs_Large_##id##.dds"/>
    </CConsoleSkin>
    <CConsoleSkin default="1" id="ConsoleBase_Reward" parent="ConsoleBase_Standard">
